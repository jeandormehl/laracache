{
    "name": "jeandormehl/laracache",
    "license": "MIT",
    "description": "InterSystems Cach√© provider for Laravel (ODBC)",
    "keywords": [
        "laravel",
        "intersystems",
        "cache",
        "pdo",
        "odbc"
    ],
    "homepage": "https://github.com/jeandormehl/laracache",
    "authors": [
        {
            "name": "Jean Dormehl",
            "email": "jdormehl@gmail.com"
        }
    ],
    "require": {
        "php": "^8.5",
        "illuminate/database": "^12.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.8",
        "pestphp/pest": "^4.1",
        "phpunit/phpunit": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "Laracache\\": "src/"
        }
    },
    "scripts": {
        "test": "pest --compact --no-coverage",
        "test:coverage": "php -d pcov.enabled=1 ./vendor/bin/pest --coverage --compact",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v2.0-dev"
        },
        "laravel": {
            "providers": [
                "Laracache\\Cache\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
